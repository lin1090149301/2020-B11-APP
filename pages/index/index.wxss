#style2 {
  margin-left: 20px;
}
.searchbar-result {
  margin-top: 0;
  font-size: 14px;
}
.searchbar-result .weui-cell__bd {
  padding: 2px 0 2px 20px;
  color: var(--weui-FG-1);
}
.searchbar-result:before {
  display: none;
}

#search {
  height: 20px;
  width: 750rpx;
  position: fixed;
  top: 0;
  z-index: 200;
}

#head {
  height: 10px;
  background-color: white;
  width: 90%;
  position: fixed;
  top: 54px;
  z-index: 200;
  border-bottom: 1px solid #ededed;
}

.content {
  height: 100px;
  background-color: #ededed;
}

.weui-media-box weui-media-box_appmsg {
  height: 33%;
}

.weui-media-box__hd {
  height: 100%;
  width: 40%;
}

.weui-media-box__thumb {
  width: 100%;
  height: 100%;
}

.weui-grids {
  background-color: #ededed;
}

.weui-grid {
  background-color: white;
  width: 360rpx;
  height: 200px;
  display: flex; /*设置显示样式*/
  align-items: center; /*垂直居中*/
  flex-direction: column; /*row--垂直 column--水平 */
  border-radius: 10px;
  margin-left: 10rpx;
  margin-top: 10rpx;
}

.weui-grid__icon {
  width: 70%;
  height: 80%;
  display: flex; /*设置显示样式*/
  align-items: center; /*垂直居中*/
  flex-direction: column; /*row--垂直 column--水平 */
}

#img2 {
  height: 100%;
  width: 100%;
}

.weui-grid__label {
  text-align: center;
}

#add {
  padding: 20px;
  background-color: #ededed;
  border: none;
}
